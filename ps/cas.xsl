               <div class="main" id="main">                        <xsl:variable name="source">                            <xsl:value-of select="cas/title" />                        </xsl:variable>                        <h2>                            <xsl:value-of select="cas/title" />                        </h2>                        <xsl:if test="string-length(cas/advisor)!=0">                            <div class="info">                                Advisor: <xsl:value-of select="cas/advisor" />                            </div>                        </xsl:if>                        <xsl:if test="cas/imagelist">                            <br />                            <div id="holder" align="center" >                                <div id="wrapper" >                                    <div class="slider-wrapper theme-default">                                        <div id="slider" class="nivoSlider">                                            <xsl:for-each select="cas/imagelist/imagegroup">                                                <img>                                                    <xsl:attribute name="src">                                                        ../images/<xsl:value-of select="image"/>                                                    </xsl:attribute>                                                    <xsl:attribute name="alt">                                                        <xsl:value-of select="image"/>                                                    </xsl:attribute>                                                    <xsl:if test="string-length(caption)!=0">                                                        <xsl:attribute name="title">                                                            <xsl:value-of select="caption"/>                                                        </xsl:attribute>                                                    </xsl:if>                                                </img>                                            </xsl:for-each>                                        </div>                                    </div>                                </div>                            </div>                        </xsl:if>                        <xsl:for-each select="cas/content">                            <h3>                                <xsl:value-of select="headline"/>                            </h3>                            <xsl:for-each select="subcontent">                                <xsl:if test="string-length(subheadline)!=0">                                    <h4>                                        <xsl:value-of select="subheadline" />                                    </h4>                                </xsl:if>                                <xsl:for-each select="main">                                    <xsl:choose>                                        <xsl:when test="@maintype = 'normal'">                                            <p>                                                <div class="description" style="text-indent: 40px;">                                                    <xsl:value-of select="." disable-output-escaping="yes"/>                                                </div>                                                                                </p>                                        </xsl:when>                                        <xsl:when test="@maintype = 'list'">                                            <li style="margin-left:30px;">                                                <div class="description">                                                    <xsl:value-of select="." disable-output-escaping="yes"/>                                                </div>                                             </li>                                        </xsl:when>                                        <xsl:when test="@maintype = 'corner'">                                            <div class="corner">                                                <div class="description">                                                    <xsl:value-of select="." disable-output-escaping="yes"/>                                                </div>                                                </div>                                        </xsl:when>                                        <xsl:when test="@maintype = 'noindent'">                                            <p class="noindent">                                                                                                <div class="description">                                                    <xsl:value-of select="." disable-output-escaping="yes"/>                                                </div>                                              </p>                                        </xsl:when>                                        <xsl:otherwise>                                        </xsl:otherwise>                                    </xsl:choose>                                </xsl:for-each>                            </xsl:for-each>                        </xsl:for-each>                    </div>                </div>                <div class="footer">                    <p>St. Mary's International School IB Website - 2015 </p>                </div>                <script type="text/javascript" src="/javascript/jquery-1.9.0.min.js"></script>                <script type="text/javascript" src="/javascript/jquery.nivo.slider.js"></script>                <script type="text/javascript">                    $(window).load(function() {                    $('#slider').nivoSlider();                    });                </script>            </body>        </html>    </xsl:template></xsl:stylesheet>